{
  "name": "serverless-librato-alerts",
  "version": "1.0.9",
  "engines": {
    "node": ">= 12"
  },
  "dependencies": {
    "lodash": "~4.17.21",
    "node-fetch": "~2.6.2"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.172",
    "@types/node-fetch": "^2.5.12",
    "@types/serverless": "1.78.35",
    "@typescript-eslint/eslint-plugin": "4.31.1",
    "@typescript-eslint/parser": "4.31.1",
    "eslint": "7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^14.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsdoc": "^36.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-security": "^1.4.0",
    "husky": "7.0.2",
    "lint-staged": "11.1.2",
    "markdownlint-cli": "~0.28.1",
    "npm-run-all": "^4.1.5",
    "pinst": "2.1.6",
    "prettier": "2.4.0",
    "prettier-plugin-packagejson": "2.2.12",
    "typescript": "4.4.3"
  },
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "lint:markdown": "prettier --write '*.md' '!(node_modules|dist|example)/**/*.md' && markdownlint '*.md' '!(node_modules|dist|example)/**/*.md' --config=.github/linters/.markdown-lint.yml --fix",
    "lint:code": "prettier --write *.json .eslintrc.js .prettierrc.js && eslint --fix --ext .ts src",
    "lint": "run-p lint:*",
    "lint-staged": "lint-staged",
    "prepublishOnly": "rm -rf dist && npm run build && npm run lint && pinst --disable",
    "postinstall": "husky install",
    "postpublish": "pinst --enable"
  },
  "lint-staged": {
    "*.md": [
      "markdownlint --config=.github/linters/.markdown-lint.yml --fix"
    ],
    "./*.js": [
      "eslint --fix"
    ],
    "*.ts": [
      "eslint --fix"
    ]
  },
  "repository": "https://github.com/mpirik/serverless-librato-alerts.git"
}
